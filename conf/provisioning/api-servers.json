{
  "project": "ThingSPIN Companion API Servers",
  "creator": "lonycell",
  "version": "1.0",
  "ignore": false,
  "servers": [
    {
      "name": "API Example Node",
      "api": "api/hello",
      "enable": true,
      "url": "http://localhost:5555",
      "run": true,
      "shell": "node",
      "pwd": "data/api-servers/hello",
      "cmd": "api.hello.js",
      "params": "--port 5555"
    },
    {
      "name": "NodeRED Complex Event Processing",
      "api": "api/cep",
      "enable": true,
      "url": "http://localhost:1880",
      "attachURL": true,
      "run": false
    },
    {
      "name": "ThingSPIN Local MQTT Broker",
      "api": "thingspin-proxy/mqtt",
      "enable": true,
      "url": "ws://localhost:1884",
      "attachURL": true,
      "run": false
    }
  ]
}
